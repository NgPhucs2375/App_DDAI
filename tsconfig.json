{
  "extends": "expo/tsconfig.base", 
  "compilerOptions": {
    "jsx": "react-native",
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "moduleResolution": "nodenext",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}